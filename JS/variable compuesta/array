<script>
const colors = ["Red", "Blue", "Green", "White"];

let text = "<ul>";
colors.forEach(list);
text += "</ul>";
document.getElementById("demo").innerHTML = text;

function list(value) {
  text += "<li>" + value + "</li>";
} 
